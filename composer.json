{
    "name": "stephanecoinon/softswitch",
    "description": "PHP client for Soft-Switch PBX API",
    "license": "MIT",
    "authors": [
        {
            "name": "St√©phane Coinon",
            "email": "me@stephcoinon.dev"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.2",
        "php": "^7.4"
    },
    "require-dev": {
        "larastan/larastan": "^1.0",
        "mockery/mockery": "^1.2",
        "orchestra/testbench": "^5.0",
        "pestphp/pest": "^1.23",
        "phpstan/phpstan-mockery": "^1.1",
        "phpunit/phpunit": "^9.0",
        "vlucas/phpdotenv": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "StephaneCoinon\\SoftSwitch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "StephaneCoinon\\SoftSwitch\\Laravel\\SoftSwitchServiceProvider"
            ],
            "aliases": {
                "SoftSwitch": "StephaneCoinon\\SoftSwitch\\Laravel\\SoftSwitch"
            }
        }
    },
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true,
            "pestphp/pest-plugin": true
        },
        "sort-packages": true
    }
}
